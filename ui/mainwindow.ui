	background-color: #58c3c5;
	border: 1px inset #48b3b5;
}
QPushButton:disabled {
	background-color: rgb(103, 115, 122);
	color: #999;
}


QTabWidget::pane { /* selecciona la ventana, sin las pestañas */
	border-top: 2px solid #0b7e7f;
	margin-top: -1px;
}
QTabWidget::tab-bar {
	left: 5px;
}

QTabBar::tab { /* selecciona la barra de las pestañas */
	background-color: #0b7e7f;
	border: 1px solid #0b7e7f;
	margin: 0 1px;
	color: #fff;
	padding: 2px;
	width: 150px;
	height: 20px;
	font-size: 18px;
}
QTabBar::tab:hover {
	background-color: #35bc88;
	border: 1px solid #20ae7c;
}
QTabBar::tab:selected {
	margin-bottom: -1px;
	background-color: qlineargradient(spread:pad, x1:0.658, y1:1, x2:0.289, y2:0, stop:0 rgba(11, 126, 127, 255), stop:1 rgba(84, 137, 172, 255));
	border-top-color:  rgba(84, 137, 172, 255);
}
QTabBar::tab:!selected {
	margin-top: 3px;
	height: 17px;
}


QTableView {
	background-color: #58dfab;
