	padding-right: 2px;
	padding-left: 2px;
}


#label_nameWarning,
#label_stockWarning,
#label_categoryWarning,
#label_unitPriceWarning,
#label_comercialPriceWarning {
	background-color: #F65755;
	color: #fff;
	border-radius: 5px;
}


/* lineedits */
QLineEdit {
	background-color: #e0e1dd;
	color: #0d1b2a;
	border: none;
	border-radius: 10px;
	padding-left: 3px;
	padding-right: 3px;
}
QLineEdit:focus {
	background-color: #3b66ab;
	color: #fff;
}


/* botones */
QPushButton {
	background-color: #415a77;
	color: #fff;
	border: none;
	border-radius: 4px;
	width: 220px;
	height: 30px;
}
QPushButton:hover,
QPushButton:focus {
	background-color: #3b66ab;
}
QPushButton:pressed {
	background-color: #3b66ab;
	border: 1px inset #778da9;
}
QPushButton:disabled {
	background-color: rgb(103, 115, 122);
	color: #999;
}


/* QComboBoxes */
QComboBox {
	background-color: #e0e1dd;
	color: #0d1b2a;
	border: 1px solid #3b66ab;
	border-radius: 5px;
	padding-left: 2px 0;
}
QComboBox:on {
	background-color: #3b66ab;
	color: #fff;
	border: none;
	padding-top: 3px;
	padding-left: 4px;
}
QComboBox::drop-down {
	subcontrol-origin: padding;
	subcontrol-position: top right;
	width: 20px;
	padding-right: 3px;
	border-left: none;
	border-top-right-radius: 5px;
	border-bottom-right-radius: 5px;
}
QComboBox::down-arrow:on {
    top: 1px;
    left: 1px;
}
QComboBox QAbstractItemView{
	background-color: #778da9;
	color: #fff;
	selection-background-color: #3b66ab;
}


/* scrollbars */
QScrollBar {
	background-color: #e0e1dd;
	border: 1px solid transparent;
	border-radius: 5px;
}
QScrollBar:groove {
	border-radius: 5px;
}
QScrollBar::handle {
	background-color:  #1b263b;
	border-radius: 5px;
}
QScrollBar::handle:pressed {
	background-color: #3b465b;
}
QScrollBar::sub-line {
	width: 0;
	height: 0;
	background: none;
}
QScrollBar::add-line {
	width: 0;
	height: 0;
	background: none;
}


/*vertical scrollbars*/
QScrollBar:vertical {
	width: 13px;
}
QScrollBar::handle:vertical {
	min-height: 15px;
}
QScrollBar::sub-page:vertical {
	background: none;
}
QScrollBar::add-page:vertical {
	background: none;
}</string>
