	color: #0d1b2a;
}


QDialog {
	background-color: #e0e1dd;
}


/* frames */
QFrame, QWidget {
	border: none;
}


#frame_upper,
#frame_middle,
#frame_lower {
	background-color: #fff;
	padding: 0px 3px;
	border-radius: 10px;
}


/* labels */
